@prefix : <https://mvdk.digital/schema/mvdko/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix mvdko: <https://mvdk.digital/schema/mvdko/> .
@base <https://mvdk.digital/schema/mvdko/> .

<https://mvdk.digital/schema/mvdko/> rdf:type owl:Ontology ;
                                      owl:versionIRI <https://mvdk.digital/schema/mvdko/> ;
                                      owl:imports <http://www.w3.org/2004/02/skos/core> ;
                                      rdfs:comment """Intern verwendetes Datenmodell für das \"Mediatheken der Darstellenden Kunst digital vernetzen\" Projekt. Das primäre Anwendungsgebiet des Datenmodells sind Audiovisuellen Medien in der Domäne der Darstellende Künste

In aktiver Bearbeitung."""@de .

#################################################################
#    Annotation properties
#################################################################


#################################################################
#    Datatypes
#################################################################


#################################################################
#    Object Properties
#################################################################

###  https://mvdk.digital/schema/mvdko/hasAlternativeLabel
mvdko:hasAlternativeLabel rdf:type owl:ObjectProperty ;
                          rdfs:domain owl:Thing ,
                                      mvdko:Agent ,
                                      mvdko:CreativeConcept ,
                                      mvdko:Event ,
                                      mvdko:MediaResource ,
                                      mvdko:Person ;
                          rdfs:range rdf:langString .


###  https://mvdk.digital/schema/mvdko/hasAlternativeTitle
mvdko:hasAlternativeTitle rdf:type owl:ObjectProperty ;
                          rdfs:subPropertyOf mvdko:hasAlternativeLabel ;
                          rdfs:domain mvdko:CreativeConcept ,
                                      mvdko:Event ,
                                      mvdko:MediaResource .


###  https://mvdk.digital/schema/mvdko/hasContributor
mvdko:hasContributor rdf:type owl:ObjectProperty ;
                     rdfs:domain mvdko:CreativeConcept ,
                                 mvdko:Event ,
                                 mvdko:MediaCarrier ,
                                 mvdko:MediaResource ;
                     rdfs:range mvdko:Agent ;
                     rdfs:comment "mvdk empfiehlt, (mvdko:hasContributor)-Beziehungen via RDF-Star mit (mvdko:hasFunction) und/oder (mvdko:hasRole) weiter zu spezifizieren."@de ,
                                  "mvdk empfiehlt, wenn genauere Angaben der Beziehung zwischen Medienressource und des*der Mitwirkenden bekannt sind, eine der beiden Properties (mvdko:hasFunction) und/oder (mvdko:hasRole) für die genauere Beschreibung der Mitwirkungsbeziehung (mvdko:hasContributor) zu nutzen."@de ,
                                  "mvdk recommends further specifying (mvdko:hasContributor)-relationships via RDF-Star with (mvdko:hasFunction) and/or (mvdko:hasRole)."@en ,
                                  "mvdk recommends using one of the two properties (mvdko:hasFunction) and/or (mvdko:hasRole) to describe the relationship of contribution more precisely (mvdko:hasContributor) if more details are known about the relationship between the Media Resource and the Contributor."@en ;
                     rdfs:label "has contributor"@en ,
                                "hat Mitwirkende*n"@de ;
                     skos:definition "Beschreibt die Mitwirkung von Akteur*innen an einem Kreativen Konzept, einem Ereignis oder einer Medienressource."@de ,
                                     "Describes the contribution of Agents involved in a Creative Concept, an Event or a Media Resource."@en ;
                     skos:example "Am Workshop \"Mediatheken vernetzen II\" (mvdko:Event) wirkte (mvdko:hasContributor) das \"Projektteam von Mediatheken der Darstellenden Kunst digital vernetzen\" (mvdko:Agent) mit."@de ,
                                  "An der Dokumentation \"Breath Made Visible\" (mvdko:MediaResource) wirkte (mvdko:hasContributor) \"Ruedi Gerber\" (mvdko:Person) mit."@de ,
                                  "An der Produktion \"Violet\" (mvdko:CreativeConcept) wirkten (mvdko:hasContributor) \"Varinia Canto Vila\", \"Adam Linder\", \"Kotomi Nishiwaki\", \"Roger Sala Reyner\", \"Meg Stuart\" und viele weitere (mvdko:Person) mit."@de ,
                                  "At the documentary \"Breath Made Visible\" (mvdko:MediaResource) contributed (mvdko:hasContributor) \"Ruedi Gerber\" (mvdko:Person)."@en ,
                                  "At the production \"Violet\" (mvdko:CreativeConcept) contributed (mvdko:hasContributor) \"Varinia Canto Vila\", \"Adam Linder\", \"Kotomi Nishiwaki\", \"Roger Sala Reyner\", \"Meg Stuart\" and many others (mvdko:Person)."@en ,
                                  "At the workshop \"Mediatheken vernetzen II\" (mvdko:Event) contributed (mvdko:hasContributor) the \"Projektteam von Mediatheken der Darstellenden Kunst digital vernetzen\" (mvdko:Agent)."@en ;
                     skos:scopeNote "Es können mehrere Mitwirkungsbeziehungen bestehen."@de ,
                                    "There can be several relationships of contribution."@en .


###  https://mvdk.digital/schema/mvdko/hasEncodingFormat
mvdko:hasEncodingFormat rdf:type owl:ObjectProperty ;
                        rdfs:subPropertyOf owl:topObjectProperty ;
                        rdfs:domain mvdko:MediaCarrier .


###  https://mvdk.digital/schema/mvdko/hasEvent
mvdko:hasEvent rdf:type owl:ObjectProperty ;
               rdfs:domain mvdko:CreativeConcept ,
                           mvdko:MediaResource ;
               rdfs:range mvdko:Event ;
               rdfs:label "has event"@en ,
                          "hat Ereignis"@de ;
               skos:definition "Relates a Media Resource or a Creative Concept to an Event."@en ,
                               "Verbindet eine Medienressource oder ein Kreatives Konzept mit einem Ereignis."@de ;
               skos:example "Die Aufzeichnung (mvdko:MediaResource) zeigt die am 22.02.2023 stattgefundene (mvdko:hasEvent) Aufführung von \"In den Alpen/Après les Alpes\" (mvdko:Event) am \"Volkstheater Wien\"."@de ,
                            "Die Produktion \"Faust\" (mvdko:CreativeConcept) von \"Einar Schleef\" wurde aufgeführt am (mvdko:hasEvent) 16.10.1993."@de ,
                            "The production \"Faust\" (mvdko:CreativeConcept) by \"Einar Schleef\" took place (mvdko:hasEvent) 16.10.1993."@en ,
                            "The recording (mvdko:MediaResource) shows the performance (mvdko:hasEvent) of \"In den Alpen/Après les Alpes\" (mvdko:Event) which took place on 22.02.2023 at the \"Volkstheater Wien\"."@en .


###  https://mvdk.digital/schema/mvdko/hasFunction
mvdko:hasFunction rdf:type owl:ObjectProperty ;
                  rdfs:domain mvdko:Agent .


###  https://mvdk.digital/schema/mvdko/hasKeyword
mvdko:hasKeyword rdf:type owl:ObjectProperty ;
                 rdfs:domain owl:Thing .


###  https://mvdk.digital/schema/mvdko/hasLanguage
mvdko:hasLanguage rdf:type owl:ObjectProperty ;
                  rdfs:domain mvdko:CreativeConcept ,
                              mvdko:Event ,
                              mvdko:MediaResource .


###  https://mvdk.digital/schema/mvdko/hasMediaCarrier
mvdko:hasMediaCarrier rdf:type owl:ObjectProperty ;
                      rdfs:domain mvdko:MediaResource ;
                      rdfs:range mvdko:MediaCarrier ;
                      rdfs:label "has media carrier"@en ,
                                 "hat Medienträger"@de ;
                      skos:definition "Links Media Resources to their corresponding Media Carriers."@en ,
                                      "Verbindet eine Medienressource mit dem physischen oder digitalen Objekt, das sie trägt."@de ;
                      skos:example "Der Mitschnitt von \"Faust\" (mvdko:MediaResource) von \"Einar Schleef\", aufgeführt 1993 auf den Stufen des \"Schillertheaters\", ist auf (hasMediaCarrier) einer VHS-Kassette (mvdko:MediaCarrier) festgehalten."@de ,
                                   "Die mp4-Datei mit einer Aufzeichnung der Performance \"stroke all the colours out of the sky\" (mvdko:Mediaresource) von \"Asher O’Gorman\" , aufgeführt 2023 am \"brut nordwest\", ist auf (hasMediaCarrier) einer externen Festplatte (mvdko:MediaCarrier) gespeichert."@de ,
                                   "The mp4 recording of the performance \"stroke all the colours out of the sky\" (mvdko:Mediaresource) by \"Asher O'Gorman\", performed in 2023 at \"brut nordwest\", is stored on (hasMediaCarrier) an external hard drive (mvdko:MediaCarrier)."@en ,
                                   "The recording of \"Faust\" (mvdko:MediaResource) by \"Einar Schleef\", performed in 1993 on the steps of the \"Schillertheater\", is recorded on (hasMediaCarrier) a VHS cassette (mvdko:MediaCarrier)."@en .


###  https://mvdk.digital/schema/mvdko/hasPart
mvdko:hasPart rdf:type owl:ObjectProperty ;
              rdfs:domain owl:Thing ;
              rdfs:range owl:Thing ;
              rdfs:comment "Organisationen, die aus (mehreren) Akteur*innen bestehen, werden mit diesen Akteur*innen über die Eigenschaft (mvdko:hasContributor) verbunden, nicht über (mvdko:hasPart)."@de ,
                           "Organizations that consist of (multiple) Agents, should be linked with them using the (mvdko:hasContributor) property, not using (mvdko:hasPart)."@en ;
              rdfs:label "has part"@en ,
                         "hat Teil"@de ;
              skos:definition "Eine generische Verbindung einer Entität mit einem zu ihr gehörigen Teil."@de ,
                              "Links an entity generically with a corresponding part of it."@en ;
              skos:example "Austria (mvdko:Place) contains (mvdko:hasPart) the city of Vienna (mvdko:Place)."@en ,
                           "Der Workshop \"tanz digital - Technologien, Methoden, künstlerische Strategien\" (mvdko:Event) hat als Teilveranstaltung (mvdko:hasPart) das Panel \"Introduction of digital technologies in dance\" (mvdko:Event)."@de ,
                           "Die Dokumentation \"Der lange stille Weg\" (mvdko:MediaResource) hat einen Teil (mvdko:hasPart) \"Der lange stille Weg: Dialoge\" (mvdko:MediaResource)."@de ,
                           "The documentary \"Der lange stille Weg\" (mvdko:MediaResource) has a part (mvdko:hasPart) \"Der lange stille Weg: Dialoge\" (mvdko:MediaResource)."@en ,
                           "The workshop \"tanz digital - Technologien, Methoden, künstlerische Strategien\" (mvdko:Event) included a panel (mvdko:hasPart) named \"Introduction of digital technologies in dance\" (mvdko:Event)."@en ,
                           "Österreich (mvdko:Place) enthält (mvdko:hasPart) die Stadt Wien (mvdko:Place)."@de .


###  https://mvdk.digital/schema/mvdko/hasPlace
mvdko:hasPlace rdf:type owl:ObjectProperty ;
               rdfs:domain mvdko:Agent ,
                           mvdko:CreativeConcept ,
                           mvdko:Event ,
                           mvdko:MediaResource ;
               rdfs:range mvdko:Place ;
               rdfs:comment "mvdk empfiehlt, bei Vorhandensein spezifischerer Informationen sollte eine der vorhandenen Unterklassen genutzt werden."@de ,
                            "mvdk recommends that if more specific information is available, one of the existing subclasses should be used."@en ;
               rdfs:label "has place"@en ,
                          "hat Ort"@de ;
               skos:definition "Drückt eine generische Beziehung zu einem Ort aus."@de ,
                               "This property represents a generic relation to some Place."@en ;
               skos:example "Barrie Kosky performed his \"Dreigroschenoper\" (mvdko:CreativeConcept) at the (mvdko:hasPlace) \"Berliner Ensemble\" (mvdko:Place)."@en ,
                            "Barrie Kosky zeigte seine \"Dreigroschenoper\" (mvdko:CreativeConcept) am (mvdko:hasPlace) \"Berliner Ensemble\" (mvdko:Place)."@de ,
                            "Das \"Schubert Theater\" (mvdko:Organization) ist situiert (mvdko:hasPlaceOfResidence) in Wien (mvdko:Place)."@de ,
                            "Die Aufführung des Theaterstücks \"Faust\" (mvdko:Event) fand statt im (mvdko:hasPlace) \"Schillertheater\" in Berlin (mvdko:Place)."@de ,
                            "The \"Schubert Theater\" (mvdko:Organization) is located (mvdko:hasPlaceOfResidence) in Vienna (mvdko:Place)."@en ,
                            "The performance of the play \"Faust\" (mvdko:Event) took place at the (mvdko:hasPlace) \"Schillertheater\" in Berlin (mvdko:Place)."@en .


###  https://mvdk.digital/schema/mvdko/hasPlaceOfOccurrence
mvdko:hasPlaceOfOccurrence rdf:type owl:ObjectProperty ;
                           rdfs:subPropertyOf mvdko:hasPlace ;
                           rdfs:domain mvdko:Event ;
                           rdfs:range mvdko:Place ;
                           rdfs:label "has place of occurrence"@en ,
                                      "hat Ereignisort"@de ;
                           skos:definition "Beschreibt den Ort, an dem ein Ereignis stattfindet."@de ,
                                           "Describes the Place where an Occurrence takes place."@en ;
                           skos:example "Der Workshop \"Mediatheken vernetzen II\" (mvdko:Event) fand statt (mvdko:hasPlace) in der Mediathek für Tanz und Theater (mvdko:Place)."@de ,
                                        "Die Uraufführung von \"Elfriede Jelineks\" \"Schwarzwasser\" (mvdko:Event) fand statt (mvdko:hasPlace) am \"Burgtheater (Akademietheater) Wien\" (mvdko:Place)."@de ,
                                        "The premiere of \"Schwarzwasser\" (mvdko:Event) by \"Elfriede Jelinek\" took place at (mvdko:hasPlace) \"Burgtheater (Akademietheater) Wien\" (mvdko:Place)."@en ,
                                        "The workshop \"Mediatheken vernetzen II\" (mvdko:Event) took place (mvdko:hasPlace) in the Mediathek für Tanz und Theater (mvdko:Place)."@en .


###  https://mvdk.digital/schema/mvdko/hasPlaceOfPublication
mvdko:hasPlaceOfPublication rdf:type owl:ObjectProperty ;
                            rdfs:subPropertyOf mvdko:hasPlace ;
                            rdfs:domain mvdko:CreativeConcept ,
                                        mvdko:MediaResource ;
                            rdfs:range mvdko:Place ;
                            rdfs:label "has Place of Publication"@en ,
                                       "hat Veröffentlichungsort"@de ;
                            skos:definition "Connects an entity to a Place where it is published."@en ,
                                            "Verbindet eine Entität mit einem Ort ihrer Veröffentlichung."@de ;
                            skos:example "Das Buch \"Performing the Archive\" (mvdko:CreativeConcept) wurde im \"Olms-Verlag\" in Hildesheim (mvdko:Place) veröffentlicht (mvdko:hasPlaceOfPublication)."@de ,
                                         "Die als DVD veröffentlichte Dokumentation \"Breath Made Visible\" (mvdko:MediaResource) hat den Veröffentlichungsort (mvdko:hasPlaceOfPublication) Nortorf (mvdko:Place)."@de ,
                                         "The book \"Performing the Archive\" (mvdko:CreativeConcept) was published (mvdko:hasPlaceOfPublication) by \"Olms-Verlag\" in Hildesheim (mvdko:Place)."@en ,
                                         "The documentary \"Breath Made Visible\" (mvdko:MediaResource) published als DVD has the place of publication (mvdko:hasPlaceOfPublication) Nortorf (mvdko:Place)."@en ;
                            skos:scopeNote "Beschreibt den Veröffentlichungsort von Entitäten wie (mvdko:CreativeConcept) und (mvdko:MediaResource)."@de ,
                                           "Describes the place of publication of entities such as (mvdko:CreativeConcept) and (mvdko:MediaResource)."@en .


###  https://mvdk.digital/schema/mvdko/hasPlaceOfResidence
mvdko:hasPlaceOfResidence rdf:type owl:ObjectProperty ;
                          rdfs:subPropertyOf mvdko:hasPlace ;
                          rdfs:domain mvdko:Agent ;
                          rdfs:range mvdko:Place ;
                          rdfs:label "has place of residence"@en ,
                                     "hat Residenzort"@de ;
                          skos:definition "Beschreibt den Ort, an dem ein Ensemble oder ein*e Künstler*in residiert."@de ,
                                          "Describes the Place where an ensemble or artist resides."@en ;
                          skos:example "Das Ensemble des \"Schubert Theater\" (mvdko:Organization) ist situiert (mvdko:hasPlaceOfResidence) in Wien (mvdko:Place)."@de ,
                                       "Das Performance-Kollektiv \"She She Pop\" (mvdko:Organization) ist (mvdko:hasPlaceOfResidence) in Berlin (mvdko:Place) ansässig."@de ,
                                       "The ensemble of the \"Schubert Theater\" (mvdko:Organiziation) is located (mvdko:hasPlaceOfResidence) in Vienna (mvdko:Place)."@en ,
                                       "The performance collective \"She She Pop\" (mvdko:Organization) is based (mvdko:hasPlaceOfResidence) in Berlin (mvdko:Place)."@en .


###  https://mvdk.digital/schema/mvdko/hasProvider
mvdko:hasProvider rdf:type owl:ObjectProperty ;
                  rdfs:domain mvdko:MediaCarrier ,
                              mvdko:MediaResource ;
                  rdfs:range mvdko:Agent ;
                  rdfs:label "has provider"@en ,
                             "hat Bestandshalter*in"@de ;
                  skos:definition "Beschreibt den*die Bestandshalter*in einer Entität wie einer Medienressource oder eines Medienträgers."@de ,
                                  "Describes the provider of an entity such as a Media Resource or a Media Carrier."@en ;
                  skos:example "Die DVD \"Breath made visible\" (mvdko:MediaCarrier) findet sich im Bestand von (mvdko:hasProvider) der \"Universitätsbibliothek Bayreuth\" (mvdko:Organization)."@de ,
                               "Die Medienresource \"Die Hamletmaschine Musiktheater in fünf Teilen (1983-1986)\" (mvdko:MediaResource) hat den*die Bestandshalter*in (mvdko:hasProvider) \"Institut für Angewandte Theaterwissenschaft der Justus-Liebig-Universität Gießen\" (mvdko:Organization)."@de ,
                               "The DVD \"Breath made visible\" (mvdko:MediaCarrier) can be found in the collection (mvdko:hasProvider) of the \"Universitätsbibliothek Bayreuth\" (mvdko:Organization)."@en ,
                               "The Media Resource \"Die Hamletmaschine Musiktheater in fünf Teilen (1983-1986)\" (mvdko:MediaResource) has the provider (mvdko:hasProvider) \"Institut für Angewandte Theaterwissenschaft der Justus-Liebig-Universität Gießen\" (mvdko:Organization)."@en .


###  https://mvdk.digital/schema/mvdko/hasPublicationChannel
mvdko:hasPublicationChannel rdf:type owl:ObjectProperty ;
                            rdfs:domain mvdko:MediaResource ;
                            rdfs:range mvdko:Organization .


###  https://mvdk.digital/schema/mvdko/hasRecordingOf
mvdko:hasRecordingOf rdf:type owl:ObjectProperty ;
                     rdfs:domain mvdko:MediaResource ;
                     rdfs:range mvdko:Agent ,
                                mvdko:CreativeConcept ,
                                mvdko:Event ;
                     rdfs:label "has recording of"@en ,
                                "hat Aufzeichnung von"@de ;
                     skos:definition "Links a Media Resource with an entity of which it contains a recording."@en ,
                                     "Verbindet eine Medienressource mit einer Entität, von der sie eine Aufzeichnung enthält."@de ;
                     skos:example "Das Video \"Buddy Bodies\" (mvdko:MediaResource) enthält eine Aufzeichnung (mvdko:hasRecordingOf) des Stücks \"Buddy Bodies\" (mvdko:CreativeConcept)."@de ,
                                  "Die Audiodatei \"Verdrängt das Kino das Theater?\" (mvdko:MediaResource) enthält eine Aufzeichnung (mvdko:hasRecordingOf) eines Gesprächs zwischen \"Theodor W. Adorno\", \"Martin Walser\", \"Joachim Kaiser\" und \"Uwe Nettelbeck\" (mvdko:Event)."@de ,
                                  "The Video \"Buddy Bodies\" (mvdko:MediaResource) includes a recording of (mvdko:hasRecordingOf) the play \"Buddy Bodies\" (mvdko:CreativeConcept)."@en ,
                                  "The audio file \"Verdrängt das Kino das Theater?\" (mvdko:MediaResource) includes a recording of (mvdko:hasRecordingOf) a conversation between \"Theodor W. Adorno\", \"Martin Walser\", \"Joachim Kaiser\" and \"Uwe Nettelbeck\" (mvdko:Event)."@en .


###  https://mvdk.digital/schema/mvdko/hasRegionalCode
mvdko:hasRegionalCode rdf:type owl:ObjectProperty ;
                      rdfs:domain mvdko:MediaCarrier .


###  https://mvdk.digital/schema/mvdko/hasRightsHolder
mvdko:hasRightsHolder rdf:type owl:ObjectProperty ;
                      rdfs:domain mvdko:CreativeConcept ,
                                  mvdko:MediaCarrier ,
                                  mvdko:MediaResource ;
                      rdfs:range mvdko:Agent .


###  https://mvdk.digital/schema/mvdko/hasRole
mvdko:hasRole rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf owl:topObjectProperty ;
              rdfs:domain mvdko:Agent .


###  https://mvdk.digital/schema/mvdko/hasTelevisionStandard
mvdko:hasTelevisionStandard rdf:type owl:ObjectProperty ;
                            rdfs:domain mvdko:MediaCarrier ,
                                        mvdko:MediaResource .


###  https://mvdk.digital/schema/mvdko/hasType
mvdko:hasType rdf:type owl:ObjectProperty ;
              rdfs:domain owl:Thing .


#################################################################
#    Data properties
#################################################################

###  https://mvdk.digital/schema/mvdko/hasAlternativeLabel
mvdko:hasAlternativeLabel rdf:type owl:DatatypeProperty ;
                          rdfs:subPropertyOf owl:topDataProperty .


###  https://mvdk.digital/schema/mvdko/hasAlternativeTitle
mvdko:hasAlternativeTitle rdf:type owl:DatatypeProperty ;
                          rdfs:range rdf:langString ,
                                     xsd:string .


###  https://mvdk.digital/schema/mvdko/hasBegin
mvdko:hasBegin rdf:type owl:DatatypeProperty ;
               rdfs:domain mvdko:Event ;
               rdfs:range xsd:dateTime ;
               rdfs:label "has begin"@en ,
                          "hat Beginn"@de ;
               skos:definition "Beschreibt den zeitlichen Beginn einer Entität."@de ,
                               "Describes the temporal beginning of an entity."@en ;
               skos:example "Die Aufführung (mvdko:Event) des \"Hamlet\" (mvdko:CreativeConcept) startet (mvdko:hasBegin) um 19:00."@de ,
                            "Die Öffnung der Berliner Mauer (mvdko:Event) begann (mvdko:hasBegin) am 9.11.1989 mit der Pressekonferenz von \"Günther Schabowski\"."@de ,
                            "The opening of the Berlin Wall (mvdko:Event) began (mvdko:hasBegin) on 9.11.1989 with the press conference by \"Günther Schabowski\"."@en ,
                            "The performance of \"Hamlet\" (mvdko:CreativeConcept) starts (mvdko:hasBegin) at 19:00."@en ;
               skos:scopeNote "Is used to describe the beginning of a time period, if such a temporal assignment can be made. The entity may or may not have an end (mvdko:hasEnd)."@en ,
                              "Wird verwendet zur Beschreibung des Beginns einer Zeitspanne, falls solch eine Zeitzuordnung vorgenommen werden kann.. Die Entität kann aber muss nicht ein Ende (mvdko:hasEnd) haben."@de .


###  https://mvdk.digital/schema/mvdko/hasDate
mvdko:hasDate rdf:type owl:DatatypeProperty ;
              rdfs:domain mvdko:Agent ,
                          mvdko:CreativeConcept ,
                          mvdko:Event ,
                          mvdko:MediaCarrier ,
                          mvdko:MediaResource ;
              rdfs:range rdf:langString ,
                         xsd:dateTime ;
              rdfs:label "has date information"@en ,
                         "hat Datumsangabe"@de ;
              skos:definition "Links a concrete date, a specific moment in time or a period of time to an entity."@en ,
                              "Verbindet ein konkretes Datum, einen einzelnen Zeitpunkt oder eine Zeitspanne mit einer Entität."@de ;
              skos:example "Die Aufführung der Inszenierung \"Ophelia's got talent\" (mvdko:Event) fand statt am (mvdko:hasDate) 15.09.2022, 19:30-22:30."@de ,
                           "Die Aufführung der Produktion \"Violet\" (mvdko:Event) fand statt am (mvdko:hasDate) 13.05.2022."@de ,
                           "Die Geburt von \"Virginia Woolf\" (mvdko:Event) war am (mvdko:hasDate) 25.01.1882."@de ,
                           "The birth of \"Virginia Woolf\" (mvdko:Event) took place on (mvdko:hasDate) 25.01.1882."@en ,
                           "The performance of the production \"Ophelia's got talent\" (mvdko:Event) took place on (mvdko:hasDate) 15.09.2022, 19:30-22:30."@en ,
                           "The performance of the production \"Violet\" (mvdko:Event) took place on (mvdko:hasDate) 13.05.2022."@en ;
              skos:scopeNote "Includes all Date information. Further specification of the type of Date (for example: (hasDateOfPublication)) should be made if known."@en ,
                             "Umfasst alle Datumsangaben. Nähere Spezifikation der Art der Datumsangabe (zum Beispiel: (hasDateOfPublication)) sollte, wenn bekannt, vorgenommen werden."@de .


###  https://mvdk.digital/schema/mvdko/hasDateOfPublication
mvdko:hasDateOfPublication rdf:type owl:DatatypeProperty ;
                           rdfs:subPropertyOf mvdko:hasDate ;
                           rdfs:domain mvdko:CreativeConcept ,
                                       mvdko:MediaResource ;
                           rdfs:range xsd:dateTime ;
                           rdfs:label "has date of publication"@en ,
                                      "hat Veröffentlichungsdatum"@de ;
                           skos:definition "Beschreibt das Veröffentlichungsdatum einer Entität."@de ,
                                           "Describes the Date of Publication of an entity."@en ;
                           skos:example "Das Buch \"Ästhetik des Performativen\" (mvdko:MediaResource) von \"Erika Fischer-Lichte\" (mvdko:hasContributor) wurde veröffentlicht (mvdko:hasDateOfPublication) im Jahr 2004."@de ,
                                        "Die audiovisuelle Dokumentation \"Breath Made Visible\" (mvdko:MediaResource) wurde als DVD veröffentlicht am (mvdko:hasDateOfPublication) 25.11.2010."@de ,
                                        "The audiovisual documentary \"Breath Made Visible\" (mvdko:MediaResource) was published as a DVD on (mvdko:hasDateOfPublication) 25.11.2010."@en ,
